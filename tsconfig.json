{
    "extends": "@tsconfig/node16-strictest-esm",
    "ts-node": {
        "preferTsExts": true
    },
    "compilerOptions": {
        "resolveJsonModule": true,
        "diagnostics": true,
        "moduleResolution": "Node",
        "importsNotUsedAsValues": "preserve",
        "allowSyntheticDefaultImports": true,
        "module": "CommonJS",
    }, "include": ["**/*.json", "**/*.ts"],
    "exclude": ["**/*.d.ts"]
}
